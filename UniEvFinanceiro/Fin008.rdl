  <DataSets>
    <DataSet Name="cdAluno">
      <Query>
        <DataSourceName>LyceumDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_aluno">
            <Value>=Parameters!p_aluno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	A.ALUNO,
	A.NOME_COMPL,
	C.CURSO + ' - ' + C.NOME AS CURSO,
	A.TURNO,
	A.CURRICULO,
	A.SERIE,
	A.TURMA_PREF
	--* 
FROM LY_ALUNO A
	INNER JOIN VW_CURSO C ON A.CURSO = C.CURSO
WHERE A.ALUNO = @p_aluno</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ALUNO">
          <DataField>ALUNO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NOME_COMPL">
          <DataField>NOME_COMPL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CURSO">
          <DataField>CURSO</DataField>
