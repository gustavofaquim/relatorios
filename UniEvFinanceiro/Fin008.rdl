    <DataSet Name="cdMatricula">
      <Query>
        <DataSourceName>LyceumDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_aluno">
            <Value>=Parameters!p_aluno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	G.SERIE_IDEAL,
	MP.DISCIPLINA,
	D.NOME,
	MP.SIT_MATRICULA,
	D.CREDITOS
FROM
	LY_ALUNO A
	INNER JOIN VW_MATR_E_PRE_MATR_RESUMO MP ON MP.ALUNO = A.ALUNO 
	INNER JOIN  LY_CURRICULO CC ON A.CURRICULO = CC.CURRICULO AND CC.CURSO = A.CURSO AND CC.TURNO = A.TURNO
	INNER JOIN LY_GRADE G ON G.CURRICULO = CC.CURRICULO AND G.TURNO = CC.TURNO AND G.CURSO = CC.CURSO AND A.SERIE = G.SERIE_IDEAL AND G.DISCIPLINA = MP.DISCIPLINA
	INNER JOIN LY_DISCIPLINA D ON D.DISCIPLINA = MP.DISCIPLINA
WHERE MP.ALUNO = @p_aluno</CommandText>
      </Query>
      <Fields>
        <Field Name="SERIE_IDEAL">
          <DataField>SERIE_IDEAL</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DISCIPLINA">
          <DataField>DISCIPLINA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NOME">
          <DataField>NOME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SIT_MATRICULA">
          <DataField>SIT_MATRICULA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CREDITOS">
          <DataField>CREDITOS</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="cdGrade">
      <Query>
        <DataSourceName>LyceumDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_aluno">
            <Value>=Parameters!p_aluno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	G.SERIE_IDEAL,
	G.DISCIPLINA,
	D.NOME,
	D.CREDITOS
FROM LY_ALUNO A
	INNER JOIN  LY_CURRICULO CC ON A.CURRICULO = CC.CURRICULO AND CC.CURSO = A.CURSO AND CC.TURNO = A.TURNO
	INNER JOIN LY_GRADE G ON G.CURRICULO = CC.CURRICULO AND G.TURNO = CC.TURNO AND G.CURSO = CC.CURSO AND A.SERIE = G.SERIE_IDEAL
	INNER JOIN LY_DISCIPLINA D ON D.DISCIPLINA = G.DISCIPLINA
WHERE ALUNO  = @p_aluno</CommandText>
      </Query>
      <Fields>
        <Field Name="SERIE_IDEAL">
