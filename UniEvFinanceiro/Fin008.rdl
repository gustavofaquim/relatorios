        <DataSourceName>LyceumDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_aluno">
            <Value>=Parameters!p_aluno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  
	V.SEMESTRALIDADE,
